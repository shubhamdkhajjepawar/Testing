<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>56.0</apiVersion>
    <description>Launches flow to look for existing contact and create new if none found</description>
    <environments>Default</environments>
    <formulas>
        <name>raiseredgeid</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!$Record.Raiser_s_Edge_ID__c}), &quot;NO RE ID&quot;, {!$Record.Raiser_s_Edge_ID__c})</expression>
    </formulas>
    <interviewLabel>Contact Lookup - Caregiver Recognition {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Contact Lookup - Caregiver Recognition</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Launch_Flow</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Contact__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <filters>
            <field>Type_of_Nomination__c</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>NCCY</stringValue>
            </value>
        </filters>
        <filters>
            <field>Team_Name__c</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <object>Caregiver_Recognition__c</object>
        <recordTriggerType>Create</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Active</status>
    <subflows>
        <name>Launch_Flow</name>
        <label>Launch Flow</label>
        <locationX>176</locationX>
        <locationY>335</locationY>
        <flowName>Find_Contact_as_Caregiver_Recognition</flowName>
        <inputAssignments>
            <name>varAccountID</name>
            <value>
                <elementReference>$Record.Account__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>varCaregiverRecognitionID</name>
            <value>
                <elementReference>$Record.Id</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>varHonoreeFirstName</name>
            <value>
                <elementReference>$Record.Honoree_First_Name__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>varHonoreeLastName</name>
            <value>
                <elementReference>$Record.Honoree_Last_Name__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>varHonoreeSuffix</name>
            <value>
                <elementReference>$Record.Honoree_Suffix__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>varHonoreeTitle</name>
            <value>
                <elementReference>$Record.Honoree_Title__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <name>varRaisersEdgeID</name>
            <value>
                <elementReference>raiseredgeid</elementReference>
            </value>
        </inputAssignments>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </subflows>
</Flow>
