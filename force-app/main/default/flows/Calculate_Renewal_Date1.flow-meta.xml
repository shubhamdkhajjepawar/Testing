    <description>Calculates the renewal date on membership.</description>
    <environments>Default</environments>
    <formulas>
        <name>Formula</name>
        <dataType>Date</dataType>
        <expression>IF(
AND(
MONTH({!$Record.Renewal_Date__c})=MONTH(TODAY()),
DAY({!$Record.Membership_Start_Date__c})=DAY(TODAY())
),
DATE(YEAR(TODAY())+1,MONTH({!$Record.Membership_Start_Date__c}),DAY({!$Record.Membership_Start_Date__c})),
IF(
DATE(YEAR(TODAY()),MONTH({!$Record.Membership_Start_Date__c}),DAY({!$Record.Membership_Start_Date__c}))&gt;
TODAY(),
DATE(YEAR(TODAY()),MONTH({!$Record.Membership_Start_Date__c}),DAY({!$Record.Membership_Start_Date__c})),
IF(
DATE(YEAR(TODAY()),MONTH({!$Record.Membership_Start_Date__c}),DAY({!$Record.Membership_Start_Date__c}))&lt;
TODAY(),
DATE(YEAR(TODAY())+1,MONTH({!$Record.Membership_Start_Date__c}),DAY({!$Record.Membership_Start_Date__c})),
NULL)))</expression>
    </formulas>
    <formulas>
