TODAY(),
DATE(YEAR(TODAY()),MONTH({!$Record.Overridden_Renewal_Date__c}),DAY({!$Record.Overridden_Renewal_Date__c})),
IF(
DATE(YEAR(TODAY()),MONTH({!$Record.Overridden_Renewal_Date__c}),DAY({!$Record.Overridden_Renewal_Date__c}))&lt;
TODAY(),
DATE(YEAR(TODAY())+1,MONTH({!$Record.Overridden_Renewal_Date__c}),DAY({!$Record.Overridden_Renewal_Date__c})),
NULL)))</expression>
    </formulas>
